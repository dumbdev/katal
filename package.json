{
  "name": "katal",
  "version": "1.0.2",
  "description": "A simple yet powerful web framework for Bun with TypeScript",
  "license": "MIT",
  "homepage": "https://github.com/dumbdev/katal#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dumbdev/katal"
  },
  "bugs": "https://github.com/dumbdev/katal/issues",
  "author": "Syed Najam Abbas (https://najamabbas.me)",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "scripts": {
    "dev": "bun run examples/simple/app.ts",
    "example": "bun run examples/simple/app.ts",
    "build": "bun run build.ts",
    "prepublishOnly": "bun run build",
    "test": "bun test",
    "lint": "bunx tsc --noEmit && bunx eslint . --ext .ts",
    "format": "bunx prettier --tab-width 4 --write ."
  },
  "keywords": [
    "bun",
    "framework",
    "web",
    "typescript",
    "mvc",
    "controllers",
    "middleware",
    "validation",
    "auth"
  ],
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/bun": "latest",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "bun-plugin-dts": "^0.3.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "bun": ">=1.0.0"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}
